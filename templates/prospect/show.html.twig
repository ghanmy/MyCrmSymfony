{% extends 'base.html.twig' %}

{% block title %}Prospect{% endblock %}

{% block body %}
    <h1>Prospect</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ prospect.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ prospect.name }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ prospect.type }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ prospect.address }}</td>
            </tr>
            <tr>
                <th>Tvacode</th>
                <td>{{ prospect.tvacode }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{{ prospect.createdat ? prospect.createdat|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('prospect_index') }}">back to list</a>

    <a href="{{ path('prospect_edit', {'id': prospect.id}) }}">edit</a>

    {{ include('prospect/_delete_form.html.twig') }}
{% endblock %}
